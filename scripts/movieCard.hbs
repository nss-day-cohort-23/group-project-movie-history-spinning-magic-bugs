<div class= "movieObject" id ={{id}}>
    <div class ="title">{{title}}</div>
    <div class = "year">{{release_date}}</div>
    <div class = "cast">{{cast}}</div>
    <div class = "poster">
        {{#if poster_path}}
        <img src = https://image.tmdb.org/t/p/w342{{poster_path}}>
        {{else}}
        <img src = https://learn.getgrav.org/user/pages/11.troubleshooting/01.page-not-found/error-404.png>
        {{/if}}
    </div>
    <div class="rate" id="${+movieObject.id}"></div>
    <div class = "watched" class= "movieObject" id ={{id}}></div>
    
    {{!-- <button type="button">Delete</button> --}}
    <button type="button" class="watchlist">Add to Watchlist</button>
</div>
